#!/bin/bash
#SBATCH --job-name=seq_matmul
#SBATCH --output=seq_matmul.out
#SBATCH --error=seq_matmul.err
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1
#SBATCH --time=00:10:00
#SBATCH --mem=3GB

for i in 1000 2000 3000 4000 5000 6000 7000 8000 9000 10000
do
    ./main $i $i $i 10
done