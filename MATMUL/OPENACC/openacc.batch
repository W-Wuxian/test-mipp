#!/bin/bash
#SBATCH --job-name=openacc_matmul
#SBATCH --output=openacc_matmul.out
#SBATCH --error=openacc_matmul.err
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --gres=gpu:1
#SBATCH --time=00:10:00
#SBATCH --mem=3GB

module load cuda

for i in 1000 2000 3000 4000 5000 6000 7000 8000 9000 10000
do
    ./main $i $i $i $10
done
